@model IEnumerable<VehicleEnquiryTechnicalTest.Models.CarTypes>

@{
    ViewBag.Title = "Car Type";
    Layout = "~/Views/Shared/_Layout.cshtml";
}


<h2>Colour</h2>

<p>
    @Html.ActionLink("Create New", "CarTypeCreate")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.CarType)
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.CarType)
            </td>
            <td>
                @Html.ActionLink("Edit", "CarTypeEdit", new { id = item.ID }) |
                @*@Html.ActionLink("Details", "CarTypeDetail", new { id = item.ID }) |*@
                @Html.ActionLink("Delete", "CarTypeDelete", new { id = item.ID })
            </td>
        </tr>
    }

</table>




